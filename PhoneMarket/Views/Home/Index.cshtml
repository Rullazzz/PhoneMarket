@using PhoneMarket.Domain.Entity
@model IEnumerable<Phone>

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <table class="table">
    @foreach (var p in Model)
    {
        <tr>
            <td>@p.Name</td>
            <td>@p.Model</td>
            <td>@p.TypeOperatingSystem</td>
            <td>@p.Price</td>
        </tr>
    }
    </table>
</div>
