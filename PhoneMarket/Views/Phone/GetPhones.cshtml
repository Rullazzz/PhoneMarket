@using PhoneMarket.Domain.Entity

@model IEnumerable<Phone>

@{
    ViewData["Title"] = "Phones";
}

<h2>Phones</h2>

<div class="text-center">
    <table class="table">
    @foreach (var p in Model)
    {
        <tr>
            <td>@p.Name</td>
            <td>@p.TypePhone</td>
            <td>@p.TypeOperatingSystem</td>
            <td>@p.Price</td>
        </tr>
    }
    </table>
</div>

